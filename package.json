{
  "name": "customer-feedback-app",
  "version": "1.0.0",
  "description": "Customer feedback application.",
  "main": "server.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "lt -p 5000 -s ueiahifsqot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephenDK/customer-feedback-app.git"
  },
  "author": "Stephen Klein",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StephenDK/customer-feedback-app/issues"
  },
  "homepage": "https://github.com/StephenDK/customer-feedback-app#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "localtunnel": "^1.9.0",
    "mongoose": "^5.0.12",
    "nodemon": "^1.17.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^5.8.0"
  }
}
